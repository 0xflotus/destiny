  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^25.1.2",
    "@types/mkdirp": "^1.0.0",
    "@types/node": "^13.7.0",
    "@types/resolve": "^1.14.0",
    "@types/rimraf": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": ">=4",
    "jest": "^25.1.0",
    "lint-staged": ">=10",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "tree-node-cli": "^1.2.5",
    "typescript": "^3.7.5"
  },
  "license": "MIT",
  "dependencies": {
    "commander": "^4.1.1",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "resolve": "^1.15.1",
    "simple-git": "^1.131.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/tests/fixtures/",
      "/test/tmp/"
    ],
    "coveragePathIgnorePatterns": [
      "/tests/tmp/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts}": "prettier --write",
    "*.ts": "eslint --cache --fix"
  }
